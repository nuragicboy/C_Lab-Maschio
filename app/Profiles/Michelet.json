      },
      {
        "action": "header"
      },
      {
        "action": "removeDBDuplicates",
        "DBColumn": "Codice Laboratorio",
        "dataColumn":"Codice"
      },
      {
        "action": "translateColumns"
      },
      {
        "action": "delColByName",
        "columns": [
          "Attività",
