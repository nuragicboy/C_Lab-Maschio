{
  "Michelet": {
    "options": {
      "datasource": "XLS/XLSX",
      "header": false,
      "TranslateColumnsBeforeLoading": false,
      "AllowMismatchedColumns": false,
      "AddProfileColumn": true,
      "ProfileColumn": "Laboratorio",
      "AddTimestamp": true,
      "TimestampColumn": "Data Inserimento",
      "target": "Maschio"
    },
    "steps": [
      {
        "action": "readExcel"
      },
      {
        "action": "transpose"
      },
      {
        "action": "drop",
        "rownum": 1
      },
      {
        "action": "header"
      },
      {
        "action": "translateColumns"
      },
      {
        "action": "delColByName",
        "columns": [
          "Attività",
          "Attività - Matrice",
          "Descrizione",
          "Locazione",
          "Prodotto",
          "Assegnato"
        ]
      }
    ]
  },
  "Enochimico": {
    "options": {
      "datasource": "XLS/XLSX",
      "header": true,
      "TranslateColumnsBeforeLoading": false,
      "AllowMismatchedColumns": false,
      "AddProfileColumn": true,
      "ProfileColumn": "Laboratorio",
      "AddTimestamp": true,
      "TimestampColumn": "Data Inserimento",
      "target": "Maschio"
    },
    "steps": [
      {
        "action": "readExcel"
      },
      {
        "action": "drop",
        "rownum": 0
      }
    ]
  },
  "UIV": {
    "options": {
      "datasource": "CSV",
      "header": true,
      "TranslateColumnsBeforeLoading": false,
      "AllowMismatchedColumns": false,
      "AddProfileColumn": true,
      "ProfileColumn": "Laboratorio",
      "AddTimestamp": true,
      "TimestampColumn": "Data Inserimento",
      "target": "Maschio"
    },
    "steps": [
      {
        "action": "readCSV",
        "SkipRows": "1"
      },
      {
        "action": "drop",
        "rownum": 1
      },
      {
        "action": "removeRowOnRegex",
        "expression": "^[0-9]{2}VR[0-9]{5}$",
        "column": "a"
      }
    ]
  }
}
